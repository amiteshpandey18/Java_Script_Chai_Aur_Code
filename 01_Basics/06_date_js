let date = new Date();

console.log(date);
console.log(date.toString());
console.log(date.toDateString());
console.log(date.toLocaleDateString());

console.log(typeof date); // object

let myCreatedDate = new Date(2023,0,18,5,30);
console.log(myCreatedDate.toLocaleString());

let myTimeStamp = Date.now();

console.log(myTimeStamp); // it will give number of milliseconds passed since 1 January 1970.

console.log(myCreatedDate.getTime());  // returns the number of milliseconds between 1 January 1970 and myCreatedDate.

console.log(Math.floor(Date.now()/1000));  // returns the current Unix timestamp in seconds.

let newDate = new Date();

console.log(newDate);
console.log(newDate.getDay()+1);
console.log(newDate.getMonth());
